{
  "name": "qwertz-fr",
  "description": "Swiss French QWERTZ layout",
  "geometry": "iso",
  "keymap": {
    "Backquote":      ["§", "°"],
    "Digit1":         ["1", "+", "¦"],
    "Digit2":         ["2", "\"", "@"],
    "Digit3":         ["3", "*", "#"],
    "Digit4":         ["4", "ç", "°"],
    "Digit5":         ["5", "%", "§"],
    "Digit6":         ["6", "&", "¬"],
    "Digit7":         ["7", "/", "|"],
    "Digit8":         ["8", "(", "¢"],
    "Digit9":         ["9", ")"],
    "Digit0":         ["0", "="],
    "Minus":          ["'", "?", "´"],
    "Equal":          ["*^", "*`", "*~"],
    "KeyQ":           ["q", "Q"],
    "KeyW":           ["w", "W"],
    "KeyE":           ["e", "E", "€"],
    "KeyR":           ["r", "R"],
    "KeyT":           ["t", "T"],
    "KeyY":           ["z", "Z"],
    "KeyU":           ["u", "U"],
    "KeyI":           ["i", "I"],
    "KeyO":           ["o", "O"],
    "KeyP":           ["p", "P"],
    "BracketLeft":    ["è", "È", "["],
    "BracketRight":   ["*¨", "*¨", "]"],
    "KeyA":           ["a", "A"],
    "KeyS":           ["s", "S"],
    "KeyD":           ["d", "D"],
    "KeyF":           ["f", "F"],
    "KeyG":           ["g", "G"],
    "KeyH":           ["h", "H"],
    "KeyJ":           ["j", "J"],
    "KeyK":           ["k", "K"],
    "KeyL":           ["l", "L"],
    "Semicolon":      ["é", "É"],
    "Quote":          ["à", "À", "{"],
    "Backslash":      ["$", "£", "}"],
    "IntlBackslash":  ["<", ">", "\\"],
    "KeyZ":           ["y", "Y"],
    "KeyX":           ["x", "X"],
    "KeyC":           ["c", "C"],
    "KeyV":           ["v", "V"],
    "KeyB":           ["b", "B"],
    "KeyN":           ["n", "N"],
    "KeyM":           ["m", "M"],
    "Comma":          [",", ";"],
    "Period":         [".", ":"],
    "Slash":          ["-", "_"],
    "Space":          [" ", " ", " ", " "]
  },
  "deadkeys": {
    "*^": {
      "*^": "^", " ": "^",
      "a": "â", "e": "ê", "i": "î", "o": "ô", "u": "û",
      "A": "Â", "E": "Ê", "I": "Î", "O": "Ô", "U": "Û"
    },
    "*¨": {
      "*¨": "¨", " ": "¨",
      "a": "ä", "e": "ë", "i": "ï", "o": "ö", "u": "ü",
      "A": "Ä", "E": "Ë", "I": "Ï", "O": "Ö", "U": "Ü"
    },
    "*`": {
      "*`": "`", " ": "`",
      "a": "à", "e": "è", "i": "ì", "o": "ò", "u": "ù",
      "A": "À", "E": "È", "I": "Ì", "O": "Ò", "U": "Ù"
    },
    "*~": {
      "*~": "~", " ": "~",
      "a": "ã", "n": "ñ", "o": "õ",
      "A": "Ã", "N": "Ñ", "O": "Õ"
    }
  },
  "altgr": true
}
